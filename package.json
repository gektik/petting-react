{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "expo start",
    "dev": "expo start --clear",
    "eas": "eas --non-interactive",
    "build:web": "expo export --platform web",
    "lint": "expo lint",
    "build:android": "EAS_NO_VCS=1 EAS_NO_CACHE=1 EAS_DISABLE_GIT_HOOKS=1 npx eas build --platform android --profile production --non-interactive --wait --retry 3",
    "build:android:local": "expo run:android --variant release",
    "prebuild": "CI=1 expo prebuild --clean",
    "build:android:preview": "EAS_NO_VCS=1 EAS_NO_CACHE=1 npx eas build --platform android --profile preview --non-interactive",
    "ios": "expo run:ios",
    "android": "expo run:android"
  },
  "dependencies": {
    "@expo/metro-runtime": "^6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@lucide/lab": "^0.1.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-google-signin/google-signin": "^15.0.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "axios": "^1.11.0",
    "expo": "^54.0.0",
    "expo-auth-session": "~7.0.8",
    "expo-blur": "~15.0.7",
    "expo-camera": "~17.0.7",
